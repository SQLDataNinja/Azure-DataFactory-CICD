{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Extract Azure SQL DB Data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "AzureSqlDBTable",
						"type": "DatasetReference",
						"parameters": {
							"param_schema": "dbo",
							"param_table": "abc"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlDBTable",
						"type": "DatasetReference",
						"parameters": {
							"param_schema": "dbo",
							"param_table": "abc"
						}
					}
				]
			},
			{
				"name": "Lookup Database Info",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "AzureSqlDBTable",
						"type": "DatasetReference",
						"parameters": {
							"param_schema": "ctrl",
							"param_table": "config_db_to_db",
							"param_db": {
								"value": "@pipeline().parameters.dp_param_db",
								"type": "Expression"
							},
							"param_dbsvr": {
								"value": "@pipeline().parameters.dp_param_dbsvr",
								"type": "Expression"
							}
						}
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Set Db List Variable",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@variables('var_db_list')",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Extract Azure SQL DB Data_copy1",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"queryTimeout": "02:00:00",
									"partitionOption": "None"
								},
								"sink": {
									"type": "AzureSqlSink",
									"preCopyScript": {
										"value": "TRUNCATE TABLE @{item().tgt_schema}.@{item().tgt_table};",
										"type": "Expression"
									},
									"disableMetricsCollection": false
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"typeConversion": true,
									"typeConversionSettings": {
										"allowDataTruncation": true,
										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
								{
									"referenceName": "AzureSqlDBTable",
									"type": "DatasetReference",
									"parameters": {
										"param_schema": {
											"value": "@{item().src_schema}",
											"type": "Expression"
										},
										"param_table": {
											"value": "@{item().src_table}",
											"type": "Expression"
										},
										"param_db": {
											"value": "@{item().src_db}",
											"type": "Expression"
										},
										"param_dbsvr": {
											"value": "@pipeline().parameters.dp_param_dbsvr",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "AzureSqlDBTable",
									"type": "DatasetReference",
									"parameters": {
										"param_schema": {
											"value": "@{item().tgt_schema}",
											"type": "Expression"
										},
										"param_table": {
											"value": "@{item().tgt_table}",
											"type": "Expression"
										},
										"param_db": {
											"value": "@{item().tgt_db}",
											"type": "Expression"
										},
										"param_dbsvr": {
											"value": "@pipeline().parameters.dp_param_dbsvr",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			},
			{
				"name": "Set Db List Variable",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Lookup Database Info",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "var_db_list",
					"value": {
						"value": "@activity('Lookup Database Info').output",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"dp_param_dbsvr": {
				"type": "string",
				"defaultValue": "test_svr"
			},
			"dp_param_db": {
				"type": "string",
				"defaultValue": "test_db"
			}
		},
		"variables": {
			"var_db_list": {
				"type": "Array"
			}
		},
		"folder": {
			"name": "Dynamic Pipelines"
		},
		"annotations": []
	}
}