						}
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlDBTable",
						"type": "DatasetReference",
						"parameters": {
							"param_schema": "dbo",
							"param_table": "abc"
						}
					}
				]
			},
			{
				"name": "Lookup Database Info",
				"type": "Lookup",
				"dependsOn": [],
